{
  "name": "@ducanh2912/react-tuby",
  "author": "napthedev, DuCanhGH",
  "description": "A React video player library with YouTube-like UI",
  "version": "0.3.8",
  "license": "MIT",
  "source": "./src/index.tsx",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/ducanh2912-react-tuby.js",
      "default": "./dist/ducanh2912-react-tuby.modern.mjs"
    },
    "./css/main.css": {
      "require": "./css/main.css",
      "import": "./css/main.css"
    }
  },
  "main": "./dist/ducanh2912-react-tuby.js",
  "module": "./dist/ducanh2912-react-tuby.module.js",
  "unpkg": "./dist/ducanh2912-react-tuby.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "css"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:main": "microbundle watch",
    "dev:style": "sass scss/main.scss css/main.css --watch --no-source-map",
    "build": "rimraf dist && rimraf css && npm run build:style && microbundle",
    "build:style": "sass scss/main.scss css/main.css --style=compressed --no-source-map",
    "test": "jest --env=jsdom",
    "lint": "eslint src --cache --cache-location ./node_modules/.cache/eslint --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "bugs": {
    "url": "https://github.com/DuCanhGH/react-tuby/issues"
  },
  "homepage": "https://github.com/DuCanhGH/react-tuby#README",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuCanhGH/react-tuby.git"
  },
  "keywords": [
    "react",
    "tuby",
    "tube",
    "youtube",
    "react-tuby"
  ],
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.modern.mjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.module.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.umd.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@size-limit/preset-small-lib": "8.1.0",
    "@size-limit/webpack": "8.1.0",
    "@size-limit/webpack-why": "8.1.0",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.0.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "microbundle": "0.15.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "sass": "1.55.0",
    "semantic-release": "19.0.5",
    "size-limit": "8.1.0",
    "ts-jest": "29.0.2",
    "tslib": "2.4.0",
    "typescript": "4.8.3",
    "webpack": "5.74.0"
  }
}
