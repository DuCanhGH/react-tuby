{
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "css"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:main": "microbundle watch",
    "dev:style": "sass scss/main.scss css/main.css --watch --no-source-map",
    "build": "microbundle && npm run build:style",
    "build:style": "sass scss/main.scss css/main.css --style=compressed --no-source-map",
    "test": "jest --env=jsdom",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "name": "@ducanh2912/react-tuby",
  "description": "A React video player library with YouTube-like UI",
  "author": "napthedev",
  "module": "dist/react-tuby.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/napthedev/react-tuby.git"
  },
  "keywords": [
    "react",
    "tuby",
    "tube",
    "youtube",
    "react-tuby"
  ],
  "size-limit": [
    {
      "path": "dist/react-tuby.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-tuby.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "8.0.1",
    "@size-limit/webpack": "8.0.1",
    "@size-limit/webpack-why": "8.0.1",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.0.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.6",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "jest": "29.0.2",
    "jest-environment-jsdom": "29.0.2",
    "microbundle": "0.15.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.54.8",
    "size-limit": "8.0.1",
    "ts-jest": "29.0.0-next.1",
    "tslib": "2.4.0",
    "typescript": "4.8.2",
    "webpack": "5.74.0"
  }
}
